# Copy this to .env for development
FRONTEND_URL=http://localhost:5173

# GitHub API (get token from https://github.com/settings/tokens)
GITHUB_TOKEN=ghp_your_github_token_here
GITHUB_ORG=your-github-org
GITHUB_API_URL=https://api.github.com

# Jira API (get token from Jira Account Settings > Security > API Tokens)
JIRA_URL=https://yourcompany.atlassian.net
JIRA_EMAIL=your-email@company.com
JIRA_TOKEN=your_jira_api_token

# AWS Cost Explorer (IAM user with Cost Explorer permissions)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1

# Railway API (get token from Railway dashboard)
RAILWAY_TOKEN=your_railway_token
RAILWAY_PROJECT_ID=your_railway_project_id
RAILWAY_API_URL=https://backboard.railway.app/graphql